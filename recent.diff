+
+        const reader = new FileReader();
+        reader.onload = (event) => {
+          try {
+            const imported = JSON.parse(event.target.result);
+
+            // Merge imported levels
+            for (const key in imported) {
+              if (this.levels.hasOwnProperty(key)) {
