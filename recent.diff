+    res.json({
+      questionId,
+      totalGraded: data.length,
+      maxPoints,
+      averageScore: Math.round(averageScore * 100) / 100,
+      averagePercentage: Math.round((averageScore / maxPoints) * 100),
+      scoreDistribution: distribution,
+      grades: data
+    });
+
+  } catch (error) {
+    console.error('Error fetching FRQ stats:', error);
