+
+          // === CLIFF EDGE HANDLE (orange) - vertical drop ===
+          ctx.strokeStyle = '#e74c3c';
+          ctx.lineWidth = 4;
+          ctx.beginPath();
+          ctx.moveTo(level.incline.endX, cliffTopY);
+          ctx.lineTo(level.incline.endX, this.groundY);
