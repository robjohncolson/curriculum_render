+      }
+
+      setStatus(text) {
+        document.getElementById('statusText').textContent = text;
+      }
+    }
+
+    // Initialize editor when page loads
+    window.addEventListener('DOMContentLoaded', () => {
+      window.levelEditor = new LevelEditor();
