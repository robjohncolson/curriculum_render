+          }
+        } else {
+          // Unit 1 - no cliff
+          room.cliffEdgeX = null;
+          room.cliffHeight = 0;
+          room.cliffTopY = null;
+          room.incline = null;
+
+          room.door = new ExitDoor(level.door.x, this.groundY - 68);
