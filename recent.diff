+
+.td-modal {
+    z-index: 10002;
+}
+
+.td-modal-content {
+    max-width: 800px;
+    width: 95%;
+    max-height: 90vh;
+    padding: 25px;
+    border-radius: 16px;
+    overflow-y: auto;
+}
+
+.td-main {
+    min-height: 400px;
+}
+
+.td-header {
+    text-align: center;
+    margin-bottom: 20px;
+}
+
+.td-header h2 {
+    margin: 0 0 5px 0;
+    font-size: 24px;
+    color: var(--text-primary);
+}
+
+.td-header p {
+    margin: 0;
+    color: var(--text-secondary);
+}
+
+/* Tabs */
+.td-tabs {
+    display: flex;
+    gap: 5px;
+    border-bottom: 2px solid var(--border-color, #e9ecef);
+    margin-bottom: 20px;
+}
+
+.td-tab {
+    padding: 12px 20px;
+    background: transparent;
+    border: none;
+    border-bottom: 3px solid transparent;
+    margin-bottom: -2px;
+    cursor: pointer;
+    font-size: 14px;
+    font-weight: 500;
+    color: var(--text-secondary);
+    transition: all 0.2s ease;
+}
+
+.td-tab:hover {
+    color: var(--text-primary);
+}
+
+.td-tab-active {
+    color: var(--primary-color, #4a90d9);
+    border-bottom-color: var(--primary-color, #4a90d9);
+}
+
+/* Table */
+.td-table {
+    width: 100%;
+    border-collapse: collapse;
+    font-size: 14px;
+}
+
+.td-table th,
+.td-table td {
+    padding: 12px;
+    text-align: left;
+    border-bottom: 1px solid var(--border-color, #e9ecef);
+}
+
+.td-table th {
+    background: var(--bg-secondary, #f8f9fa);
+    font-weight: 600;
+    color: var(--text-primary);
+}
+
+.td-table td code {
+    background: var(--bg-tertiary, #e9ecef);
+    padding: 2px 6px;
+    border-radius: 4px;
+    font-size: 13px;
+}
+
+.td-table-small {
+    font-size: 13px;
+}
+
+.td-table-small th,
+.td-table-small td {
+    padding: 8px;
+}
+
+/* Password status */
+.td-password-status {
+    padding: 4px 8px;
+    border-radius: 4px;
+    font-size: 12px;
+    font-weight: 500;
+}
+
+.td-password-status.has-password {
+    background: #dcfce7;
+    color: #166534;
+}
+
+.td-password-status.no-password {
+    background: #fef3c7;
+    color: #92400e;
+}
+
+/* Actions */
+.td-actions {
+    display: flex;
+    gap: 6px;
+}
+
+.td-btn-small {
+    padding: 6px 10px;
+    border: none;
+    border-radius: 6px;
+    background: var(--bg-secondary, #f8f9fa);
+    cursor: pointer;
+    font-size: 14px;
+    transition: all 0.2s ease;
+}
+
+.td-btn-small:hover {
+    background: var(--bg-tertiary, #e9ecef);
+}
+
+.td-btn-danger-small:hover {
+    background: #fee2e2;
+}
+
+/* Buttons */
+.td-btn {
+    padding: 10px 20px;
+    border: none;
+    border-radius: 8px;
+    font-size: 14px;
+    font-weight: 600;
+    cursor: pointer;
+    transition: all 0.2s ease;
+}
+
+.td-btn-primary {
+    background: var(--primary-color, #4a90d9);
+    color: white;
+}
+
+.td-btn-primary:hover {
+    background: var(--primary-hover, #3a7bc8);
+}
+
+.td-btn-secondary {
+    background: var(--bg-secondary, #f8f9fa);
+    color: var(--text-primary);
+    border: 1px solid var(--border-color, #e9ecef);
+}
+
+.td-btn-secondary:hover {
+    background: var(--bg-tertiary, #e9ecef);
+}
+
+.td-btn-danger {
+    background: #fee2e2;
+    color: #dc2626;
+    border: 1px solid #fecaca;
+}
+
+.td-btn-danger:hover {
+    background: #fecaca;
+}
+
+/* Footer */
+.td-footer {
+    display: flex;
+    justify-content: space-between;
+    margin-top: 25px;
+    padding-top: 20px;
+    border-top: 1px solid var(--border-color, #e9ecef);
+}
+
+/* Data tab */
+.td-data-section {
+    padding: 20px;
+    background: var(--bg-secondary, #f8f9fa);
+    border-radius: 12px;
+    margin-bottom: 15px;
+}
+
+.td-data-section h3 {
+    margin: 0 0 8px 0;
+    font-size: 16px;
+    color: var(--text-primary);
+}
+
+.td-data-section p {
+    margin: 0 0 15px 0;
+    font-size: 14px;
+    color: var(--text-secondary);
+}
+
+/* Import tab */
+.td-import-section {
+    padding: 20px;
+    background: var(--bg-secondary, #f8f9fa);
+    border-radius: 12px;
+    margin-bottom: 15px;
+}
+
+.td-import-section h3 {
+    margin: 0 0 8px 0;
+    font-size: 16px;
+    color: var(--text-primary);
+}
+
+.td-import-section p {
+    margin: 0 0 10px 0;
+    font-size: 14px;
+    color: var(--text-secondary);
+}
+
+.td-import-preview {
+    background: var(--bg-primary, #fff);
+    border: 1px solid var(--border-color, #e9ecef);
+    border-radius: 8px;
+    padding: 15px;
+    margin: 15px 0;
+    max-height: 200px;
+    overflow-y: auto;
+}
+
+.td-import-preview h4 {
+    margin: 0 0 10px 0;
+    font-size: 14px;
+}
+
+.td-import-actions {
+    display: flex;
+    gap: 10px;
+}
+
+.td-form-inline {
+    display: flex;
+    gap: 10px;
+    align-items: center;
+}
+
+.td-form-inline input {
+    flex: 1;
+    padding: 10px 12px;
+    border: 1px solid var(--border-color, #e9ecef);
+    border-radius: 6px;
+    font-size: 14px;
+}
+
+/* Empty state */
+.td-empty {
+    text-align: center;
+    padding: 40px 20px;
+    color: var(--text-secondary);
+}
+
+/* Loading */
+.td-loading {
+    text-align: center;
+    padding: 40px 20px;
+    color: var(--text-secondary);
+}
+
+/* Error */
+.td-error {
+    text-align: center;
+    padding: 40px 20px;
+    color: #dc2626;
+}
+
+/* Roster scroll */
+.td-roster {
+    max-height: 400px;
+    overflow-y: auto;
+}
+
+/* Dark theme adjustments */
+body.dark-theme .um-option-btn,
+body.dark-gray-theme .um-option-btn {
+    background: var(--bg-secondary);
+    border-color: var(--border-color);
+}
+
+body.dark-theme .um-error,
+body.dark-gray-theme .um-error {
+    background: #7f1d1d;
+    color: #fecaca;
+}
+
+body.dark-theme .td-password-status.has-password,
+body.dark-gray-theme .td-password-status.has-password {
+    background: #14532d;
+    color: #86efac;
+}
+
+body.dark-theme .td-password-status.no-password,
+body.dark-gray-theme .td-password-status.no-password {
+    background: #78350f;
+    color: #fcd34d;
+}
+
+/* Mobile responsive */
+@media (max-width: 600px) {
+    .um-modal-content {
+        padding: 20px;
+    }
+
+    .td-modal-content {
+        padding: 15px;
+    }
+
+    .td-tabs {
+        flex-wrap: wrap;
+    }
+
+    .td-tab {
+        padding: 10px 15px;
+        font-size: 13px;
+    }
+
+    .td-form-inline {
+        flex-direction: column;
+    }
+
+    .td-footer {
+        flex-direction: column;
+        gap: 10px;
+    }
+
+    .td-footer .td-btn {
+        width: 100%;
+    }
+}
